				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<FontString name="$parentTitle" inherits="LoginTitleFont" text="Debug Console">
					<Anchors><Anchor point="TOPLeft"><Offset><AbsDimension x="25" y="22"/></Offset></Anchor></Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnShow>
				getglobal(this:GetName().."EditBox"):SetFocus()
			</OnShow>
			<OnHide>
				getglobal(this:GetName().."EditBox"):ClearFocus()
				if getglobal("AccountLogin"):IsVisible() then
					getglobal("AccountLoginAccountEdit"):SetFocus()
				end
			</OnHide>
			<OnMouseDown>
				this:StartMoving();
			</OnMouseDown>
			<OnMouseUp>
				this:StopMovingOrSizing();
			</OnMouseUp>
		</Scripts>
